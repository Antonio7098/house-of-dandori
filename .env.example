# Environment / database
ENVIRONMENT=development
DATABASE_URL=postgresql://user:pass@host:5432/db

# Vector embeddings
OPENROUTER_API_KEY=sk-or-...
VECTOR_STORE_PROVIDER=chroma

# Optional overrides
# CHROMA_PERSIST_DIR=./chroma_data

# GRAPH_RAG_USE_NEO4J=true
# NEO4J_URI=bolt://localhost:7687
# NEO4J_USER=neo4j
# NEO4J_PASSWORD=change_me

# Startup behavior (leave blank to index everything)
REINDEX_ON_STARTUP=false
REINDEX_MAX_COURSES=

# Development / Debug
# DEV_BYPASS_AUTH=true
# DB_PATH=courses.db
# LOG_LEVEL=INFO

# Google Cloud Vertex AI (production)
GCP_PROJECT_ID=your-project-id
GCP_LOCATION=us-central1

# Vertex AI Vector Search V2 (Collections) - RECOMMENDED
VERTEX_AI_API_VERSION=v2
VERTEX_AI_COLLECTION_ID=your-collection-id

# Vertex AI Vector Search V1 (Index/Endpoint) - LEGACY (only if not using V2)
# VERTEX_AI_INDEX_ID=your-index-id
# VERTEX_AI_INDEX_ENDPOINT_ID=your-index-endpoint-id

# Supabase (Optional)
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_PUBLISHABLE_KEY=pk_...
# SUPABASE_SECRET_KEY=sk_...
# SUPABASE_ANON_KEY=pk_...
# SUPABASE_SERVICE_KEY=sk_...
